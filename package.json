{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "server:dev": "nodemon",
    "slack:dev": "nodemon --exec tsx api/slack/index.ts",
    "setup:database": "tsx api/slack/database/setup.ts",
    "test:database": "tsx -e \"import { DatabaseService } from './api/slack/DatabaseService.js'; import { createClient } from '@supabase/supabase-js'; const db = new DatabaseService(createClient(process.env.SUPABASE_URL, process.env.SUPABASE_SERVICE_ROLE_KEY)); console.log('Database connection test passed!');\"",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "dev:full": "concurrently \"npm run client:dev\" \"npm run server:dev\" \"npm run slack:dev\"",
    "build": "tsc",
    "start": "node dist/api/server.js",
    "start:slack": "node dist/api/slack/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:slack": "jest --testPathPattern=slack"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@octokit/rest": "^22.0.0",
    "@slack/bolt": "^3.17.1",
    "@slack/web-api": "^7.0.4",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.5",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-schedule": "^2.1.1",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "redis": "^5.8.2",
    "rss-parser": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.30",
    "@types/node-schedule": "^2.1.7",
    "@types/supertest": "^6.0.2",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^5.0.1",
    "concurrently": "^9.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
